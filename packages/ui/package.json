    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.9",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.21.0",
    "tailwindcss": "^4",
    "typescript": "5.8.2"
  },
